<p id="notice"><%= notice %></p>

<h1>Device Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Submitter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @device_brands.each do |device_brand| %>
      <tr>
        <td><%= device_brand.name %></td>
        <td><%= device_brand.submitter.name %></td>
        <td><%= link_to 'Show', device_brand %></td>
        <td><%= link_to 'Edit', edit_device_brand_path(device_brand) %></td>
        <td><%= link_to 'Destroy', device_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device Brand', new_device_brand_path %>
